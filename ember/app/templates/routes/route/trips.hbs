{{#trips-filter
        filter=(action 'filterByTrip')
        as |trips|}}
        <div class="col-md-6">
                <ul class="list-group">
                    {{#each trips as |trip|}}
                        {{trip-select trip_name=trip.trip_name}}
                    {{/each}}
                </ul>
        </div>
{{/trips-filter}}
